#! /bin/sh
# template plugin prerm script

set -e

case "$1" in
    remove|upgrade|deconfigure)
        # update the shared libs
        gstreamer-register

        ;;
    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

#DEBHELPER#

exit 0

